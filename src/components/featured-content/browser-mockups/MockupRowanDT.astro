---
import ShadowWrapper from '../../UI/ShadowWrapper.astro';
const { className, dataRellaxSpeed } = Astro.props;
---

<ShadowWrapper className={className} dataRellaxSpeed={dataRellaxSpeed}>
  <img
    src="https://kimba-imagecdn.imgix.net/kimba-design/mockup-rowan-dt-top.png?q=50&auto=format"
    data-sizes="auto"
    width="828"
    height="54"
    alt="Browser top for Rowan Clothing website"
    class="border-b border-[#dbdbdb] || lazyload"
  />
  <img
    src="https://kimba-imagecdn.imgix.net/kimba-design/mockup-rowan-dt-content.png?q=50&auto=format"
    data-sizes="auto"
    width="828"
    height="475"
    alt="Rowan Clothing website"
    class="lazyload"
  />
</ShadowWrapper>
