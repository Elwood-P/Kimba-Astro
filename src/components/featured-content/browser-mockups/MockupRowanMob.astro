---
import ShadowWrapper from '../../UI/ShadowWrapper.astro';
const { className, dataRellaxSpeed } = Astro.props;
---

<ShadowWrapper borderRadius="16px" className={className} dataRellaxSpeed={dataRellaxSpeed}>
  <img
    src="https://kimba-imagecdn.imgix.net/kimba-design/mockup-rowan-mob.png?q=50&auto=format"
    srcset="https://kimba-imagecdn.imgix.net/kimba-design/mockup-rowan-mob.png?w=204&q=75&auto=format, https://kimba-imagecdn.imgix.net/kimba-design/mockup-rowan-mob.png?q=50&auto=format 2x"
    width="204"
    height="414"
    alt="Rowan Clothing wesbite - mobile view"
    class="w-full"
  />
</MockupWrapper>
